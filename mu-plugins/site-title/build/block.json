{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "pd/site-title",
	"version": "0.1.0",
	"title": "Site Title",
	"category": "site-blocks",
	"description": "Block used to handle site titles",
	"supports": {
		"html": true,
		"color": {
			"gradients": true,
			"link": true,
			"__experimentalDefaultControls": {
				"text": true
			}
		}
	},
	"attributes": {
		"titolo": {
			"type": "string"
		},
		"uppercase": {
			"type": "boolean",
			"default": false
		},
		"tag_html": {
			"type": "string",
			"default": "h"
		},
		"tipo_heading": {
			"type": "string",
			"default": "1"
		},
		"dimensione": {
			"type": "string",
			"default": "1"
		},
		"alignment": {
			"type": "string"
		}
	},
	"textdomain": "site-title",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": "file:./style-index.css"
}
